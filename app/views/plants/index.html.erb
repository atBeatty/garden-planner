<h1>Plants Index</h1>
<ul></ul>
<% @plants.each do |plant| %>
    <li><%= link_to plant.name, plant_path(plant) %></li>
    <li><%= link_to plant.user_id, user_path(User.find(plant.user_id)) %></li>
    
    <li><%= link_to plant.garden_id, garden_path(User.find(plant.garden_id)) %></li>

<% end %>
</ul>